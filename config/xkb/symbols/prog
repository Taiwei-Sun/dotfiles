partial alphanumeric_keys
xkb_symbols "progmap" {
    replace key <BKSL>  { [ plus,   asterisk, asciicircum,  dead_macron ] };
    replace key <AC11>  { [ dead_acute, grave, dead_diaeresis, dead_diaeresis] };
    replace key <AD11>  { type[Group1]="TWO_LEVEL", [ bracketleft, braceleft] };
    replace key <AD12>  { type[Group1]="TWO_LEVEL", [ bracketright, braceright] };
};

partial modifier_keys
xkb_symbols "esc_rctrl" {
	replace key <RCTL> { [Escape] };
};

partial modifier_keys
xkb_symbols "esc_rwin" {
	replace key <RWIN> { [Escape] };
};
